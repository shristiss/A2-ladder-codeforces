#include<iostream>
using namespace std;
int main()
{
    int n,suma=0,sumb=0,sumc=0;
    cin>>n;
    int a[n],b[n],c[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i]>>b[i]>>c[i];
        suma+=a[i];
        sumb+=b[i];
        sumc+=c[i];
    }
    if(suma==0 && sumb==0 && sumc==0) cout<<"YES"<<endl;
    else cout<<"NO"<<endl;
    return 0;
}
