#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int a,b,as=0,bs=0;
    for(int i=0;i<n;i++)
    {
        cin>>a>>b;
        if(as<a && bs>b)
         cout<<"Happy Alex"<<endl;
         else if(as>0 && bs>0)
         {
             cout<<"Poor Alex";
         }
         
        as=a,bs=b;
    }
}
